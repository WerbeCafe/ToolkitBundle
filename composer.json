{
    "name": "lazyants/toolkit-bundle",
    "type": "symfony-bundle",
    "description": "Toolkit Bundle provides basic usefully functional used in most projects",
    "keywords": ["toolkit", "bundle"],
    "homepage": "https://github.com/lazyants/ToolkitBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Spivakovsky",
            "email": "maxim@lazy-ants.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.3,<2.4-dev",
        "sensio/generator-bundle": ">=2.3.3"
    },
    "require-dev": {
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "knplabs/knp-paginator-bundle": "dev-master",
        "sensio/generator-bundle": "~2.3",
        "jquery/jquery": "1.9.*"
    },
    "autoload": {
        "psr-0": {
            "Lazyants\\ToolkitBundle": ""
        }
    },
    "target-dir": "Lazyants/ToolkitBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}